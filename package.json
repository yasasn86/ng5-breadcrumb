{
  "name": "ng2-breadcrumb",
  "version": "0.5.4",
  "description": "A component that creates a breadcrumb trail for router paths",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc -w",
    "build": "tsc",
    "bundles": "tsc -m system --rootDir ./app --outFile ./bundles/ng2-breadcrumb.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gmostert/ng2-breadcrumb.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Gerhard Mostert",
  "license": "MIT",
  "main": "ng2-bootstrap.js",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/http": "^2.1.1",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "^3.1.1",
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",
    "@types/core-js": "^0.9.35",
    "@types/node": "^6.0.54",
    "@types/jasmine": "^2.5.40",
    "@types/moment": "^2.13.0",
    "@types/es6-shim": "^0.31.32"
  }
}
